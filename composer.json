{
	"name": "vhenzl/phpstan-kdyby-doctrine",
	"description": "Kdyby/Doctrine extensions for PHPStan",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "0.8-dev"
		}
	},
	"config": {
		"sort-packages": true
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/vhenzl/phpstan-doctrine"
		}
	],
	"require": {
		"php": "~7.0",
		"phpstan/phpstan": "^0.8",
		"phpstan/phpstan-doctrine": "dev-develop"
	},
	"require-dev": {
		"consistence/coding-standard": "~0.13.0",
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"phing/phing": "^2.16.0",
		"phpunit/phpunit": "^5.7",
		"slevomat/coding-standard": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"PHPStanKdybyDoctrine\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
